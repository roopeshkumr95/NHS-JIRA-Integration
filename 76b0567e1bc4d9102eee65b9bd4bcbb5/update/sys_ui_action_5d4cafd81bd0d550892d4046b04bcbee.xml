        <script><![CDATA[function resetTaskValues() {

	var tableName = g_form.getTableName();
	g_form.setMandatory('x_nhsd_nhs_jira_in_search_issue', false);
	

	var r = confirm("Do you want to reset the JIRA Information ?");
	if (r == true) {
		g_form.clearValue('x_nhsd_nhs_jira_in_issue_id');
		g_form.clearValue('x_nhsd_nhs_jira_in_issue_key');
		g_form.clearValue('x_nhsd_nhs_jira_in_issue_status');
		g_form.clearValue('x_nhsd_nhs_jira_in_issue_url');
		g_form.clearValue('x_nhsd_nhs_jira_in_jira_project');
		g_form.clearValue('x_nhsd_nhs_jira_in_search_issue');
	}
	if(tableName != 'sn_customerservice_case')
		g_form.save();
	else{
		gsftSubmit(null, g_form.getFormElement(), 'reset_task_jira_info');
	}
}

if (typeof window == 'undefined')
	saveForm();

function saveForm() {
	current.update();
	action.setRedirectURL(current);
